{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous", // Token validation/extraction happens in code
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post" // Use POST for logout actions
      ],
      "route": "logout"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
